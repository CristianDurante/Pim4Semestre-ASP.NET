@model FazendaUniao.Models.EstoqueInsumos

@{
    ViewData["Title"] = "Edit";
}

<h1>Editar Estoque Insumo</h1>


<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="EstoqueInsumosId" />
            <div class="form-group">
                <label asp-for="QuantidadeEmEstoque" class="control-label"></label>
                <input asp-for="QuantidadeEmEstoque" class="form-control" />
                <span asp-validation-for="QuantidadeEmEstoque" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Salvar" class="btn btn-success" />
                <a class="btn btn-light" asp-action="Index">Voltar</a>
            </div>
        </form>
    </div>
</div>
